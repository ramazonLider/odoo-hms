<odoo>
    <data>
        <record id="view_cart_tree" model="ir.ui.view">
            <field name="name">cart.tree</field>
            <field name="model">cart</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="user_id"/>
                    <field name="room_ids"/>
                </tree>
            </field>
        </record>

        <!-- Form View for Room Model -->
        <record id="view_cart_form" model="ir.ui.view">
            <field name="name">cart.form</field>
            <field name="model">cart</field>
            <field name="arch" type="xml">
                <form>
                    <h1 class="oe_title">Settings</h1>
                    <sheet>
                        <notebook>
                            <page string="Cart test">
                                <group>
                                    <field name="name"/>
                                    <field name="user_id"/>
                                    <field name="room_ids"/>
                                </group>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>